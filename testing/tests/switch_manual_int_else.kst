{
  "id": "switch_manual_int_else",
  "data": "switch_opcodes2.bin",
  "asserts": [
    {
      "actual": "opcodes.size",
      "expected": 4
    },
    {
      "actual": "opcodes[0].code",
      "expected": 83
    },
    {
      "actual": "opcodes[0].body.as<switch_manual_int_else::opcode::strval>.value",
      "expected": "\"foo\""
    },
    {
      "actual": "opcodes[1].code",
      "expected": 88
    },
    {
      "actual": "opcodes[1].body.as<switch_manual_int_else::opcode::noneval>.filler",
      "expected": "0x42"
    },
    {
      "actual": "opcodes[2].code",
      "expected": 89
    },
    {
      "actual": "opcodes[2].body.as<switch_manual_int_else::opcode::noneval>.filler",
      "expected": "0xcafe"
    },
    {
      "actual": "opcodes[3].code",
      "expected": 73
    },
    {
      "actual": "opcodes[3].body.as<switch_manual_int_else::opcode::intval>.value",
      "expected": 7
    }
  ]
}