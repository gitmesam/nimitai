id: default_endian_expr_inherited
data: endian_expr.bin

asserts:
  # LE
  - actual: docs[0].indicator
    expected: '[0x49, 0x49]'
  - actual: docs[0].main.insides.some_int
    expected: 0x42
  - actual: docs[0].main.insides.more.some_int1
    expected: 0x4200
  - actual: docs[0].main.insides.more.some_int2
    expected: 0x42
  - actual: docs[0].main.insides.more.some_inst
    expected: 0x42

  # BE
  - actual: docs[1].indicator
    expected: '[0x4d, 0x4d]'
  - actual: docs[1].main.insides.some_int
    expected: 0x42
  - actual: docs[1].main.insides.more.some_int1
    expected: 0x42
  - actual: docs[1].main.insides.more.some_int2
    expected: 0x4200
  - actual: docs[1].main.insides.more.some_inst
    expected: 0x42000000

  # Weird => BE
  - actual: docs[2].indicator
    expected: '[0x58, 0x58]'
  - actual: docs[2].main.insides.some_int
    expected: 0x42
  - actual: docs[2].main.insides.more.some_int1
    expected: 0x42
  - actual: docs[2].main.insides.more.some_int2
    expected: 0x4200
  - actual: docs[2].main.insides.more.some_inst
    expected: 0x42000000
